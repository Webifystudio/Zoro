# Copy this file to .env.local and fill in your credentials.
# IMPORTANT: DO NOT commit .env.local to your git repository.

# Meta App credentials for Instagram Graph API
# Get these from your app's dashboard at https://developers.facebook.com/
NEXT_PUBLIC_INSTAGRAM_APP_ID=YOUR_APP_ID_HERE
INSTAGRAM_APP_SECRET=YOUR_APP_SECRET_HERE

# This must match one of the "Valid OAuth Redirect URIs" in your Meta App settings
# (Found under Products > Instagram Basic Display > Basic Display)
NEXT_PUBLIC_REDIRECT_URI=http://localhost:9002/auth/instagram/callback
